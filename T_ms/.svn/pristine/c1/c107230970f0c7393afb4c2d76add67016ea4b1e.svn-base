 <insert id="insert" parameterType="com.bms.model.DiningTableModel">
 	insert into dining_table (dining_table_id,table_type,table_num,update_time) values(#{diningTableId},#{tableType},#{tableNum},#{updateTime})
 </insert>
 <delete id="delete" parameterType="java.util.Map">
 	delete from dining_table where dining_table_id = #{diningTbaleId}
 </delete>
 <select id="listBySearch" parameterType ="com.bms.model.DiningTableModel" resultType="java.util.Map">
 	select dining_table_id diningTableId,table_type tableType,table_num tableNum,update_time updateTime from dining_table where 1 = 1
 	<if test="tableNum != null" >
		and table_num = ${tableNum} 	
 	</if>
 	<if test="updateTime != null" >
		and update_time  like '%${updateTime}%'	
 	</if> 
 	<if test="tableType != null" >
		and table_type ='${tableType}'	
 	</if> 	
 </select>

  

